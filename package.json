{
  "name": "eleventy-critical-css",
  "version": "0.2.2",
  "description": "Eleventy plugin to extract and inline critical CSS from your HTML templates",
  "main": "index.js",
  "scripts": {
    "release": "np --no-cleanup"
  },
  "repository": "gregives/eleventy-critical-css",
  "author": "Greg Ives",
  "keywords": [
    "eleventy",
    "eleventy-plugin",
    "critical",
    "css",
    "optimization"
  ],
  "license": "MIT",
  "dependencies": {
    "critical": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "np": "^7.4.0",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
