{
  "name": "eleventy-critical-css",
  "version": "0.2.2",
  "description": "Eleventy plugin to extract and inline critical CSS from your HTML templates",
  "main": "index.js",
  "scripts": {
    "release": "np --no-cleanup"
  },
  "repository": "gregives/eleventy-critical-css",
  "author": "Greg Ives",
  "keywords": [
    "eleventy",
    "eleventy-plugin",
    "critical",
    "css",
    "optimization"
  ],
  "license": "MIT",
  "dependencies": {
    "critical": "^2.0.6"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "np": "^7.2.0",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
